#!/bin/sh

if [ "1" = "$MUSS_TEST_UP_LOGS" ] && [ "$1" = "up" ]; then
  echo log
  sleep 2 # wait long enough for status to be added
  exit 0
fi

echo std err >&2

case "$*" in
  ps\ -q\ *)
    echo "some:$3:cid"
    exit;;
  version*)
    echo fake
    exit;;
esac

echo "docker-compose"
for i in "$@"; do
  echo "$i"
done
